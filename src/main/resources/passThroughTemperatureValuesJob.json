{
  "jobId": "passThroughTemperatureValues",
  "dataSources": [
    {
      "dataSchemaIds": [
        "temperatureSchema"
      ],
      "type": "topic",
      "topicProperties": {
        "topicName": "system.devices.*.sensors.*"
      }
    }
  ],
  "transformations": {
    "engine": "system:service:analytics:engine:sqe",
    "sqeTasks": [
      {
        "query": "from passThroughTemperatureValues_temperatureSchema select temperature, timestamp, device insert into temperatureOutputStream;",
        "outputs": [
          {
            "name": "passthroughOutput",
            "dataSchemaId": "temperatureOutputSchema"
          }
        ]
      }
    ]
  },
  "exports": [
    {
      "outputName": "passthroughOutput",
      "type": "topic",
      "topicProperties": {
        "topicName": "temperature-passthrough"
      }
    }
  ]
}